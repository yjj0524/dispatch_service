{
	"easycom": {
		"^u-(.*)": "@/components/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/login/login", // 登录
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/index/index", // 首页
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/booking/booking", // 农民信息(预约)
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/bookingArrange/bookingArrange", // 农机预约安排
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/dispatchFarmer/dispatchFarmer", // 派遣农户
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/dispatchMachine/dispatchMachine", // 派遣农机
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/bookingList/bookingList", // 农机预约列表
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/dispatchService/dispatchService", // 农机调度服务
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/healthReport/healthReport", // 每日健康申报
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/statistics/statistics", // 预约调度统计
			"style": {
				"navigationStyle": "custom"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"rpxCalcMaxDeviceWidth": 1200, // rpx 计算所支持的最大设备宽度，单位 px，默认值为 960
		"rpxCalcIncludeWidth": 960, // rpx 计算特殊处理的值，始终按实际的设备宽度计算，单位 rpx，默认值为 750
		"rpxCalcBaseDeviceWidth": 750 // rpx 计算使用的基准设备宽度，设备实际宽度超出 rpx 计算所支持的最大设备宽度时将按基准宽度计算，单位 px，默认值为 375
	}
}
